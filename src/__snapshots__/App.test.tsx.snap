exports[`user journey matches snapshots 1`] = `
<div>
  <div>
    <header>
      <div>
        <h1>
          gdoc2latex GUI
        </h1>
        <p>
          Convert a Google Doc to LaTeX using 
          <a
            href="https://github.com/domdomegg/gdoc2latex"
          >
            gdoc2latex
          </a>
        </p>
      </div>
    </header>
    <section>
      <h2>
        Paste in an HTML copy of your Google Doc
      </h2>
      <p>
        Get this from Google Docs with 
        <span
          class="highlight"
        >
          File ➜ Download ➜ Web page (.html)
        </span>
        <br />
